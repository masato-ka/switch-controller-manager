<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <service>cron-task</service>
  <threadsafe>true</threadsafe>
  <runtime>java8</runtime>
  <env-variables>
    <env-var name="CRONTASK_TOPIC" value="status-notify"></env-var>
    <env-var name="CRONTASK_MESSAGE" value="kick"></env-var>
  </env-variables>
</appengine-web-app>
